<div class="row">
  <!-- Left -->
  <div class="col-xs-2">
    <div class="panel panel-default">
      <div class="panel-body">
        <p>
          <% if (current_user.avatar?) %>
            <%= image_tag current_user.avatar, :size => '100x100'%>
          <% else %>
            <%= "No Profile Picture Found" %>
          <% end %>
        </p>
      </div>
    </div>

    <div class="panel panel-default">
      <div class="panel-body">
        <p> Links </p>
        <p><a href="#">Link 1</a></p>
        <p><%= link_to "Meal Planner", showMealPlanner_path, remote: true %></p>
        <p><a href="#">Link 3</a></p>
        <p><a href="#">Link 4</a></p>
        <p><%= link_to "Support Tickets", showTickets_path, remote: true %></p>
        <p><a href="#">Link 5</a></p>
      </div>
    </div>


  </div>

  <!-- Middle -->
  <!-- Middle -->
  <div class="col-xs-10">
    <div id="current_dash_render">

    </div>
        <!-- @savedmeals is the variable in the controller that all SavedMeal database entries go into
        Is hooked up with Username as the foreign_key. Currently shows every single database entry and not profile specific-->
  </div>
</div>
</div>
